{
  "name": "@bimeister/pupakit",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://git.bimeister.com/common/pupakit.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "postinstall": "gulp fix:node_modules",
    "ng": "ng",
    "build-cli": "build-cli",
    "gulp": "gulp",
    "yarn": "yarn",
    "start": "yarn run start:demo",
    "start:demo": "yarn run create-untracked-files && ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "build": "gulp build",
    "build:dev": "yarn run create-untracked-files && ng build --watch",
    "build:demo": "yarn run create-untracked-files && ng build --project=demo",
    "build:demo-puppeteer": "gulp build-demo-static",
    "lint": "yarn run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "lint:prettier": "yarn run prettier:check",
    "prettier:format": "prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettier:check": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettify": "yarn run prettier:format && yarn run lint:debug",
    "test": "yarn run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "yarn run build:demo-puppeteer && ng test --configuration=ci",
    "autofix": "(yarn run prettify & yarn run lint:debug) && yarn run lint:inspect",
    "create-untracked-files": "gulp create:untracked-files"
  },
  "peerDependencies": {
    "@bimeister/event-bus": "^0.1.0",
    "@bimeister/utilities": "^0.1.0"
  },
  "dependencies": {
    "@angular/cdk": "~12.0.0",
    "@types/hammerjs": "^2.0.40",
    "ag-grid-angular": "^23.1.1",
    "ag-grid-community": "^23.1.1",
    "ngx-mask": "^11.0.0",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular/animations": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/language-service": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@babel/core": "~7.12.0",
    "@bimeister/build-cli": "0.1.0-c869ab81",
    "@bimeister/event-bus": "0.1.0-40a7dcbb",
    "@bimeister/utilities": "0.1.0-fe13e1f0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.0",
    "babel-loader": "^8.1.0",
    "barrelsby": "^2.2.0",
    "codelyzer": "^6.0.0",
    "combos": "^0.2.0",
    "css-loader": "^5.0.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "hammerjs": "^2.0.8",
    "jest": "^26.3.0",
    "jest-junit": "^12.0.0",
    "ng-packagr": "^12.0.0",
    "ngx-highlightjs": "^4.1.4",
    "prettier": "2.2",
    "puppeteer": "^8.0.0",
    "raw-loader": "^4.0.2",
    "replace-in-file": "^6.1.0",
    "rxjs": "^6.6.0",
    "sass": "^1.26.10",
    "scss-bundle": "^3.1.2",
    "ts-node": "^9.0.0",
    "tsickle": "~0.39.0",
    "tslint": "^6.1.3",
    "typescript": "~4.2.4"
  },
  "browser": {
    "fs": false,
    "zlib": false,
    "path": false,
    "child_process": false,
    "fancy-log": false,
    "console": false
  }
}
