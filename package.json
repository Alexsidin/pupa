  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://git.bimeister.com/common/pupakit.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "yarn": "yarn",
    "start": "yarn run start:demo",
    "start:demo": "ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "start:storybook": "start-storybook",
    "build": "gulp build",
    "build:dev": "ng build --watch",
    "build:demo": "ng build --project=demo",
    "lint": "yarn run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "lint:prettier": "yarn run prettier:check",
    "prettier:format": "prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettier:check": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettify": "yarn run prettier:format && yarn run lint:debug",
    "test": "yarn run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "ng test --configuration=ci",
    "autofix": "(yarn run prettify & yarn run lint:debug) && yarn run lint:inspect",
    "barrel": "gulp create:barrel-files"
  },
  "dependencies": {
    "@angular/cdk": "^10.1.3",
    "@meistersoft/utilities": "^0.1.0-5f00af26",
    "ag-grid-angular": "^23.1.1",
    "ag-grid-community": "^23.1.1",
    "ngx-mask": "^10.0.4",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "peerDependencies": {
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular-devkit/build-ng-packagr": "~0.1000.6",
    "@angular/animations": "~10.0.11",
    "@angular/cli": "~10.0.6",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/compiler-cli": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/language-service": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@babel/core": "^7.11.1",
    "@storybook/addon-actions": "^6.0.4",
    "@storybook/angular": "^6.0.4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.0",
    "babel-loader": "^8.1.0",
    "barrelsby": "^2.2.0",
    "codelyzer": "^6.0.0",
    "combos": "^0.2.0",
    "copy": "^0.3.2",
    "css-loader": "^4.2.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "jest": "^26.3.0",
    "ng-packagr": "^10.0.3",
    "prettier": "^2.1.2",
    "replace-in-file": "^6.1.0",
    "rxjs": "^6.6.0",
    "sass": "^1.26.10",
    "scss-bundle": "^3.1.2",
    "ts-node": "^9.0.0",
    "tsickle": "~0.38.0",
    "tslint": "^6.1.3",
    "typescript": "3.9.7"
  },
  "browser": {
    "fs": false,
    "zlib": false,
    "path": false,
    "child_process": false,
    "fancy-log": false
  }
