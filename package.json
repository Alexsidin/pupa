{
  "name": "@meistersoft/pupakit",
  "version": "0.2.0",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://git.meistersoft.ru/common/pupakit.git"
  },
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "yarn run start:demo",
    "start:demo": "ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "start:storybook": "start-storybook",
    "build": "yarn run barrel && yarn run lint && ng build && yarn run build:scss && yarn run build:css && yarn run copy:assets && yarn run replace-imports",
    "build:scss": "scss-bundle --config scss-bundle.config.json && yarn run copy:styles",
    "build:css": "sass ./dist/@meistersoft/pupakit/styles.scss ./dist/@meistersoft/pupakit/styles.css",
    "build:dev": "ng build --watch",
    "copy:assets": "copy ./src/assets/**/*.* ./dist/@meistersoft/pupakit/assets",
    "copy:styles": "copy ./src/styles/**/**/**/*.* ./dist/@meistersoft/pupakit/styles",
    "replace-imports": "replace-in-file \"/'/assets//g\" \"'~@meistersoft/pupakit/assets/\" ./dist/**/**.* --verbose --isRegex",
    "lint": "yarn run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "lint:prettier": "yarn run prettier:check",
    "prettier:format": "prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettier:check": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettify": "yarn run prettier:format && yarn run lint:debug",
    "test": "yarn run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "ng test --configuration=ci",
    "autofix": "(yarn run prettify & yarn run lint:debug) && yarn run lint:inspect",
    "barrel": "yarn run barrel:components && yarn run barrel:internal",
    "barrel:components": "barrelsby --config=\"./barrelsby.json\" --directory=\"./src/lib/components\"",
    "barrel:internal": "barrelsby --config=\"./barrelsby.json\" --directory=\"./src/internal\""
  },
  "dependencies": {
    "@angular/cdk": "^9.2.3",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "uuid": "^7.0.3",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "^0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/animations": "~9.1.2",
    "@angular/cli": "~9.1.1",
    "@angular/common": "~9.1.2",
    "@angular/compiler": "~9.1.2",
    "@angular/compiler-cli": "~9.1.2",
    "@angular/core": "~9.1.2",
    "@angular/forms": "~9.1.2",
    "@angular/language-service": "~9.1.2",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/angular": "^6.0.0-alpha.29",
    "@types/jest": "^24.0.23",
    "@types/node": "^13.0.0",
    "@types/uuid": "^7.0.0",
    "babel-loader": "^8.0.6",
    "barrelsby": "^2.2.0",
    "codelyzer": "^5.2.0",
    "combos": "^0.2.0",
    "copy": "^0.3.2",
    "css-loader": "^3.4.2",
    "ionicons": "^4.6.3",
    "jest": "^24.9.0",
    "ng-packagr": "^9.0.3",
    "prettier": "^2.0.4",
    "replace-in-file": "^4.2.0",
    "rxjs": "~6.5.5",
    "sass": "^1.26.3",
    "scss-bundle": "^3.0.1",
    "ts-node": "~8.5.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.11.1",
    "tslint": "6.1.0",
    "typescript": "^3.8.3"
  }
}