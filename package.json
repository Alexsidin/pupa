{
  "name": "@meistersoft/pupakit",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "http://bitbucket.meistersoft.ru/scm/com/pupakit.git"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build && npm run build:scss && npm run build:css && npm run copy:assets",
    "build:scss": "scss-bundle --config scss-bundle.config.json && npm run copy:variables && npm run copy:mixins && npm run copy:functions && npm run copy:fontStyles",
    "build:css": "sass ./dist/@meistersoft/pupakit/styles.scss ./dist/@meistersoft/pupakit/styles.css",
    "build:dev": "ng build --watch",
    "copy:mixins": "npx copy ./src/styles/mixins.scss ./dist/@meistersoft/pupakit",
    "copy:fontStyles": "npx copy ./src/styles/font-faces.scss ./dist/@meistersoft/pupakit",
    "copy:variables": "npx copy ./src/styles/variables.scss ./dist/@meistersoft/pupakit",
    "copy:functions": "npx copy ./src/styles/functions.scss ./dist/@meistersoft/pupakit",
    "copy:assets": "npx copy ./src/assets/**/*.* ./dist/@meistersoft/pupakit/assets",
    "test": "npm run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "ng test --configuration=ci",
    "lint": "npm run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "start": "npm run start:demo",
    "start:demo": "ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "prettify": "prettier --write ./**/*.{html,css,scss,json,js,ts} && npm run lint:debug"
  },
  "dependencies": {
    "zone.js": "~0.9.1"
  },
  "peerDependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "rxjs": "~6.5.2",
    "ionicons": "^4.6.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.4",
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "^0.801.1",
    "@angular/animations": "~8.1.1",
    "@angular/cli": "~8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "^8.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "~12.6.2",
    "codelyzer": "^5.0.0",
    "copy": "^0.3.2",
    "jest": "^24.8.0",
    "ng-packagr": "^5.3.0",
    "prettier": "^1.18.2",
    "rxjs": "~6.5.2",
    "sass": "^1.22.4",
    "scss-bundle": "^2.5.1",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "typescript": "^3.4.5",
    "ionicons": "^4.6.1",
    "combos": "^0.2.0"
  }
}
