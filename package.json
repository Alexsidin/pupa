{
  "name": "@meistersoft/pupakit",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "http://bitbucket.meistersoft.ru/scm/com/pupakit.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run start:demo",
    "start:demo": "ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "build": "npm run lint && ng build && npm run build:scss && npm run build:css && npm run copy:assets && npm run replace-imports",
    "build:scss": "scss-bundle --config scss-bundle.config.json && npm run copy:styles",
    "build:css": "sass ./dist/@meistersoft/pupakit/styles.scss ./dist/@meistersoft/pupakit/styles.css",
    "build:dev": "ng build --watch",
    "copy:assets": "npx copy ./src/assets/**/*.* ./dist/@meistersoft/pupakit/assets",
    "copy:styles": "npx copy ./src/styles/**/*.* ./dist/@meistersoft/pupakit/styles",
    "replace-imports": "npx replace-in-file \"/'/assets//g\" \"'~@meistersoft/pupakit/assets/\" ./dist/**/**.* --verbose --isRegex",
    "lint": "npm run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "lint:prettier": "npm run prettier:check",
    "prettier:format": "npx prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettier:check": "npx prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettify": "npm run prettier:format && npm run lint:debug",
    "test": "npm run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "ng test --configuration=ci"
  },
  "dependencies": {
    "ag-grid-angular": "^21.2.2",
    "ag-grid-community": "^21.2.2",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "tslib": "^1.9.0",
    "lodash": "^4.17.14",
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular-devkit/build-ng-packagr": "^0.803.6",
    "@angular/animations": "~8.2.8",
    "@angular/cli": "~8.3.6",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "^8.2.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular-devkit/build-ng-packagr": "^0.803.6",
    "@angular/animations": "~8.2.8",
    "@angular/cli": "~8.3.6",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "^8.2.8",
    "@types/jest": "^24.0.18",
    "@types/node": "~12.7.8",
    "codelyzer": "^5.1.2",
    "combos": "^0.2.0",
    "copy": "^0.3.2",
    "ionicons": "^4.6.3",
    "jest": "^24.9.0",
    "ng-packagr": "^5.5.1",
    "prettier": "^1.18.2",
    "replace-in-file": "^4.1.3",
    "rxjs": "~6.5.3",
    "sass": "^1.23.0-module.beta.1",
    "scss-bundle": "^3.0.1",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.0",
    "tslint": "~5.20.0",
    "typescript": "<3.6.0"
  }
}
