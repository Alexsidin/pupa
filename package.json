{
  "name": "@meistersoft/pupakit",
  "version": "0.2.0",
  "license": "LicenseRef-LICENSE",
  "repository": {
    "type": "git",
    "url": "http://bitbucket.meistersoft.ru/scm/com/pupakit.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run start:demo",
    "start:demo": "ng serve --project=demo --open --host 0.0.0.0 --port 4201",
    "build": "npm run barrel && npm run lint && ng build && npm run build:scss && npm run build:css && npm run copy:assets && npm run replace-imports",
    "build:scss": "npx scss-bundle --project scss-bundle.config.json && npm run copy:styles",
    "build:css": "npx sass ./dist/@meistersoft/pupakit/styles.scss ./dist/@meistersoft/pupakit/styles.css",
    "build:dev": "ng build --watch",
    "copy:assets": "npx copy ./src/assets/**/*.* ./dist/@meistersoft/pupakit/assets",
    "copy:styles": "npx copy ./src/styles/**/*.* ./dist/@meistersoft/pupakit/styles",
    "replace-imports": "npx replace-in-file \"/'/assets//g\" \"'~@meistersoft/pupakit/assets/\" ./dist/**/**.* --verbose --isRegex",
    "lint": "npm run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "lint:prettier": "npm run prettier:check",
    "prettier:format": "npx prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettier:check": "npx prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "prettify": "npm run prettier:format && npm run lint:debug",
    "test": "npm run test:debug",
    "test:debug": "ng test --configuration=debugging",
    "test:ci": "ng test --configuration=ci",
    "autofix": "(npm run prettify & npm run lint:debug) && npm run lint:inspect",
    "barrel": "npm run barrel:components && npm run barrel:internal",
    "barrel:components": "barrelsby --config=\"./barrelsby.json\" --directory=\"./src/lib/components\"",
    "barrel:internal": "barrelsby --config=\"./barrelsby.json\" --directory=\"./src/internal\"",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@angular/cdk": "^8.2.3",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.0",
    "@angular-devkit/build-angular": "^0.900.3",
    "@angular-devkit/build-ng-packagr": "~0.900.3",
    "@angular/animations": "~9.0.3",
    "@angular/cli": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@babel/core": "^7.8.6",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/angular": "^5.3.14",
    "@types/jest": "^24.0.23",
    "@types/node": "~12.12.7",
    "babel-loader": "^8.0.6",
    "barrelsby": "^2.2.0",
    "codelyzer": "^5.2.0",
    "combos": "^0.2.0",
    "copy": "^0.3.2",
    "ionicons": "^4.6.3",
    "jest": "^24.9.0",
    "ng-packagr": "^9.0.2",
    "prettier": "^1.19.1",
    "replace-in-file": "^4.2.0",
    "rxjs": "~6.5.3",
    "sass": "^1.23.3",
    "scss-bundle": "^3.0.1",
    "ts-node": "~8.5.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "typescript": "3.7.5"
  }
}
