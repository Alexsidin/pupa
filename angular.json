{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "packageManager": "npm"
  },
  "defaultProject": "pupakit",
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "development": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "./projects/demo/src/environments/environment.ts",
                  "with": "./projects/demo/src/environments/development.environment.ts"
                }
              ],
              "namedChunks": false,
              "outputHashing": "none",
              "vendorChunk": false
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "./projects/demo/src/environments/environment.ts",
                  "with": "./projects/demo/src/environments/production.environment.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/ionicons/dist/ionicons",
                "output": "/ionicons"
              },
              {
                "glob": "**/examples/**/*",
                "input": "./projects/demo/src/app/pages",
                "output": "/examples"
              },
              {
                "glob": "**/*",
                "input": "./projects/kit/src/assets/fonts",
                "output": "/assets/fonts"
              },
              {
                "glob": "**/*",
                "input": "./projects/kit/src/assets/configs",
                "output": "/assets/configs"
              },
              {
                "glob": "**/*",
                "input": "./projects/demo/src/assets",
                "output": "/assets"
              }
            ],
            "index": "./projects/demo/src/index.html",
            "main": "./projects/demo/src/main.ts",
            "outputPath": "./dist/demo",
            "polyfills": "./projects/demo/src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["./projects/kit/src/styles", "./projects/demo/src/styles", "./node_modules"]
            },
            "styles": [
              "./projects/kit/src/styles/styles.scss",
              "./node_modules/highlight.js/styles/github.css",
              "./projects/demo/src/styles/styles.scss"
            ],
            "tsConfig": "./projects/demo/tsconfig.demo.json",
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": ["fancy-log", "uuid"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "configurations": {
            "debugging": {
              "fix": true,
              "force": true,
              "quiet": true
            },
            "inspection": {
              "fix": false,
              "force": false,
              "quiet": true
            },
            "inspection-ci": {
              "fix": false,
              "force": false,
              "quiet": false
            }
          },
          "options": {
            "lintFilePatterns": ["./projects/demo/src/**/*.ts", "./projects/demo/src/**/*.html"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "aot": true,
              "browserTarget": "demo:build:development",
              "disableHostCheck": true,
              "open": true,
              "progress": true,
              "watch": true
            },
            "production": {
              "aot": true,
              "browserTarget": "demo:build:production",
              "disableHostCheck": true,
              "host": "0.0.0.0",
              "open": true,
              "port": 4200,
              "progress": true,
              "watch": false
            }
          },
          "options": {
            "browserTarget": "demo:build"
          }
        }
      },
      "prefix": "demo",
      "projectType": "application",
      "root": "./demo",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "viewEncapsulation": "Emulated",
          "changeDetection": "OnPush",
          "skipTests": true
        }
      },
      "sourceRoot": "./projects/demo/"
    },
    "pupakit": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "./ng-package.json",
            "tsConfig": "./tsconfig.lib.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "configurations": {
            "debugging": {
              "fix": true,
              "force": true,
              "quiet": true
            },
            "inspection": {
              "fix": false,
              "force": false,
              "quiet": true
            },
            "inspection-ci": {
              "fix": false,
              "force": false,
              "quiet": false
            }
          },
          "options": {
            "lintFilePatterns": ["./projects/kit/src/**/*.ts", "./projects/demo/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "configurations": {
            "ci": {
              "bail": true,
              "cache": false,
              "ci": true,
              "watch": false,
              "configPath": "jest.config.ci.js"
            },
            "debugging": {
              "bail": false,
              "cache": true,
              "ci": false,
              "watch": true,
              "configPath": "jest.config.development.js"
            }
          }
        }
      },
      "prefix": "pupa",
      "projectType": "library",
      "root": "./",
      "schematics": {
        "@schematics/angular:component": {
          "changeDetection": "OnPush",
          "export": false,
          "flat": false,
          "inlineStyle": false,
          "inlineTemplate": false,
          "skipImport": false,
          "skipTests": true,
          "style": "scss",
          "viewEncapsulation": "Emulated"
        },
        "@schematics/angular:directive": {
          "export": true,
          "flat": true,
          "skipImport": false,
          "skipTests": false
        },
        "@schematics/angular:module": {
          "commonModule": false,
          "flat": false,
          "routing": false,
          "routingScope": "Child"
        },
        "@schematics/angular:pipe": {
          "export": true,
          "flat": true,
          "skipImport": false,
          "skipTests": false
        },
        "@schematics/angular:service": {
          "flat": true,
          "skipTests": true
        }
      },
      "sourceRoot": "./projects/kit/"
    }
  },
  "version": 1
}
