{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "prefix": "demo",
      "projectType": "application",
      "root": "./demo",
      "sourceRoot": "./projects/demo/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "development": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "./projects/demo/src/environments/environment.ts",
                  "with": "./projects/demo/src/environments/development.environment.ts"
                }
              ],
              "namedChunks": false,
              "outputHashing": "none",
              "vendorChunk": false
            }
          },
          "options": {
            "assets": [
              {
                "glob": "**/examples/**/*",
                "input": "./projects/demo/src/app/pages",
                "output": "/examples"
              },
              {
                "glob": "package.json",
                "input": "./",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "./projects/common/src/assets/configs",
                "output": "/assets/configs"
              },
              {
                "glob": "**/*",
                "input": "./projects/demo/src/assets",
                "output": "/assets"
              }
            ],
            "index": "./projects/demo/src/index.html",
            "main": "./projects/demo/src/main.ts",
            "outputPath": "./projects/demo/dist",
            "polyfills": "./projects/demo/src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["./projects/common/src/styles", "./projects/demo/src/styles", "./node_modules"]
            },
            "styles": [
              "./projects/common/src/styles/styles.scss",
              "./node_modules/highlight.js/styles/github.css",
              "./projects/demo/src/styles/styles.scss"
            ],
            "tsConfig": "./projects/demo/tsconfig.demo.json",
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": ["fancy-log", "uuid"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "configurations": {
            "debugging": {
              "fix": true,
              "force": true,
              "quiet": true
            },
            "inspection": {
              "fix": false,
              "force": false,
              "quiet": true
            },
            "inspection-ci": {
              "fix": false,
              "force": false,
              "quiet": false
            }
          },
          "options": {
            "lintFilePatterns": ["./projects/demo/src/**/*.ts", "./projects/demo/src/**/*.html"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "defaultConfiguration": "development",
          "configurations": {
            "development": {
              "browserTarget": "demo:build:development",
              "disableHostCheck": true,
              "open": true,
              "host": "0.0.0.0",
              "watch": true,
              "port": 4201
            }
          },
          "options": {
            "browserTarget": "demo:build"
          }
        }
      }
    },
    "icons": {
      "projectType": "library",
      "root": "projects/icons",
      "sourceRoot": "projects/icons/src",
      "prefix": "pupa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/icons/ng-package.json",
            "tsConfig": "projects/icons/tsconfig.lib.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "configurations": {
            "debugging": {
              "fix": true,
              "force": true,
              "quiet": true
            },
            "inspection": {
              "fix": false,
              "force": false,
              "quiet": true
            },
            "inspection-ci": {
              "fix": false,
              "force": false,
              "quiet": false
            }
          },
          "options": {
            "lintFilePatterns": ["./projects/icons/src/**/*.ts"]
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "configurations": {
            "ci": {
              "bail": true,
              "cache": false,
              "ci": true,
              "watch": false,
              "configPath": "jest.config.ci.js"
            },
            "debugging": {
              "bail": false,
              "cache": true,
              "ci": false,
              "watch": true,
              "configPath": "jest.config.development.js"
            }
          },
          "options": {
            "passWithNoTests": true
          }
        }
      }
    },
